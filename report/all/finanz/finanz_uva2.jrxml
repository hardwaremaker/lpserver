	<variable name="Summe" class="java.math.BigDecimal" resetType="None">
		<variableExpression><![CDATA[($P{P_UMSATZ_INLAND_RED.STEUER}.getSaldo()
     .multiply(new BigDecimal($P{P_USTREDSTEUERSATZ}))
     .multiply(new BigDecimal(0.0100))
 .add($P{P_UMSATZ_INLAND_NORMALSTEUER}.getSaldo()
     .multiply(new BigDecimal($P{P_USTNORMALSTEUERSATZ}))
     .multiply(new BigDecimal(0.0100)))
 .add($P{P_UMSATZ_INLAND_NORMALSTEUER_ALT} != null?$V{V_STEUERBETRAG_INLAND_NORMALSTEUER_ALT}:BigDecimal.ZERO)
 .add($P{P_ANZAHLUNG_RED.STEUER}.getSaldo()
     .multiply(new BigDecimal($P{P_USTREDSTEUERSATZ}))
     .multiply(new BigDecimal(0.0100)))
 .add($P{P_ANZAHLUNG_NORMALSTEUER}.getSaldo()
     .multiply(new BigDecimal($P{P_USTNORMALSTEUERSATZ}))
     .multiply(new BigDecimal(0.0100)))
)
.subtract($P{P_VORSTEUERKONTO}.getSaldo())
.subtract($P{P_VORSTEUER_INVESTITIONEN}.getSaldo())
.subtract($P{P_IMPORT_DRITTLAND}.getSaldo())
.subtract(
            $P{P_IG_ERWERB_RED.STEUER}.getSaldo()
            .multiply(new BigDecimal($P{P_USTREDSTEUERSATZ}))
            .multiply(new BigDecimal(0.0100))
            .add($P{P_IG_ERWERB_NORMALSTEUER}.getSaldo()
                .multiply(new BigDecimal($P{P_USTNORMALSTEUERSATZ}))
                .multiply(new BigDecimal(0.0100)))
)]]></variableExpression>
