	</variable>
	<variable name="UMS_Gruppe" class="java.math.BigDecimal" resetType="Group" resetGroup="Gruppe" calculation="Sum">
		<variableExpression><![CDATA[$F{Satz}==null || $F{Umsatz} == null || $F{Kennzahl}.startsWith("---") ?
new Double(0.00) :
$F{Umsatz}.doubleValue()]]></variableExpression>
	</variable>
	<variable name="UMS_Gruppe_Steuer" class="java.math.BigDecimal" resetType="Group" resetGroup="Gruppe" calculation="Sum">
		<variableExpression><![CDATA[$F{Satz}==null || $F{Umsatz} == null || $F{Kennzahl}.startsWith("---") ?
new Double(0.00) :
(
 $F{Gruppe}.intValue() != $V{Vorsteuer}.intValue() ?
 $F{Umsatz}.doubleValue() * $F{Satz}.doubleValue() / 100 :
 $F{Umsatz}.doubleValue() * -1
)]]></variableExpression>
	</variable>
	<variable name="Summe" class="java.math.BigDecimal" resetType="None">
		<variableExpression><![CDATA[($P{P_UMSATZ_INLAND_RED.STEUER}.getSaldo()
    .multiply(new BigDecimal($P{P_USTREDSTEUERSATZ}))
    .multiply(new BigDecimal(0.0100))
.add($P{P_UMSATZ_INLAND_NORMALSTEUER}.getSaldo()
    .multiply(new BigDecimal($P{P_USTNORMALSTEUERSATZ}))
    .multiply(new BigDecimal(0.0100)))
.add($P{P_ANZAHLUNG_RED.STEUER}.getSaldo()
    .multiply(new BigDecimal($P{P_USTREDSTEUERSATZ}))
    .multiply(new BigDecimal(0.0100)))
.add($P{P_ANZAHLUNG_NORMALSTEUER}.getSaldo()
    .multiply(new BigDecimal($P{P_USTNORMALSTEUERSATZ}))
    .multiply(new BigDecimal(0.0100)))
.subtract($P{P_VORSTEUERKONTO}.getSaldo()
    .add($P{P_VORSTEUER_BETR_KFZ}.getSaldo())
    .add($P{P_VORSTEUER_BETR_GEBAEUDE}.getSaldo()))
.subtract($P{P_IMPORT_DRITTLAND}.getSaldo())
.subtract($P{P_IMPORT_DRITTLAND_ZAHLUNG_AN_FA}.getSaldo())
)]]></variableExpression>
	</variable>
	<variable name="Kammerumlage" class="java.lang.Double" resetType="None">
		<variableExpression><![CDATA[$P{P_PERIODE_KALENDERDATUM}.getYear()+1900 >= 2019 ?
new Double(0.0029) :
new Double(0.003)
/* Für Österreich gilt aus den Summen der Vorsteuer eine Kammerumlage von 0,3%
   auf die Steuern. Diese ist Quartalsweise abzuliefern
   ab 2019 gilt bis 3Mio 0,29% und ist dann degressiv siehe https://www.wko.at/service/steuern/kammerumlagen.html */]]></variableExpression>
	</variable>
