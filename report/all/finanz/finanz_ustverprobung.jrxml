	<variable name="sumBetrag" class="java.math.BigDecimal" resetType="Group" resetGroup="Steuersatz">
		<variableExpression><![CDATA[$P{P_DETAILS}
    ? $V{sumBetrag}.add($F{Detail} ? ($F{Betrag}==null?BigDecimal.ZERO:$F{Betrag}) : BigDecimal.ZERO)
    : $V{sumBetrag}.add($F{Betrag})]]></variableExpression>
		<initialValueExpression><![CDATA[BigDecimal.ZERO]]></initialValueExpression>
	</variable>
	<variable name="sumSteuer" class="java.math.BigDecimal" resetType="Group" resetGroup="Steuersatz">
		<variableExpression><![CDATA[$P{P_DETAILS}
     ? $V{sumSteuer}.add($F{Detail} ? ($F{Steuer}==null ? BigDecimal.ZERO : $F{Steuer}) : BigDecimal.ZERO)
     : $V{sumSteuer}.add($F{Steuer})]]></variableExpression>
		<initialValueExpression><![CDATA[BigDecimal.ZERO]]></initialValueExpression>
	</variable>
	<variable name="MwstSatzBerechnet" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{Betrag}!=null && $F{Betrag}.compareTo(BigDecimal.ZERO) == 0 &&
$F{Steuer}!=null && $F{Steuer}.compareTo(BigDecimal.ZERO) != 0
? new BigDecimal(100) :
(
    $F{Betrag}==null || $F{Betrag}.compareTo(BigDecimal.ZERO) == 0 ||
    $F{Steuer}==null || $F{Steuer}.compareTo(BigDecimal.ZERO) == 0
    ? BigDecimal.ZERO :
    (
        $F{Steuer}.multiply(new BigDecimal(100.00))
        .divide($F{Betrag},RoundingMode.HALF_EVEN).setScale(2,BigDecimal.ROUND_HALF_EVEN)
    )
)]]></variableExpression>
	</variable>
	<variable name="SteuerErrechnet" class="java.math.BigDecimal">
		<variableExpression><![CDATA[$F{Betrag}.multiply($F{MwstSatz}).divide(new BigDecimal(100.00),RoundingMode.HALF_EVEN).setScale(2,BigDecimal.ROUND_HALF_EVEN)]]></variableExpression>
	</variable>
	<variable name="SteuerbetragGleich" class="java.lang.Boolean">
		<variableExpression><![CDATA[!(($F{Steuer}.subtract($V{SteuerErrechnet})).abs().compareTo(new BigDecimal("0.01")) > 0)
/* ist der Steuerbetrag +/- 0,01 gleich? */]]></variableExpression>
	</variable>
	<variable name="MwstSatzGleichWieBerechnet" class="java.lang.Boolean">
		<variableExpression><![CDATA[$F{MwstSatz}.setScale(1,BigDecimal.ROUND_HALF_EVEN).compareTo(
$V{MwstSatzBerechnet}.setScale(1,BigDecimal.ROUND_HALF_EVEN)) == 0 ||
$V{SteuerbetragGleich}]]></variableExpression>
	</variable>
	<variable name="sumSteuer_Steuerart" class="java.math.BigDecimal" resetType="Group" resetGroup="Steuerart">
		<variableExpression><![CDATA[$P{P_DETAILS}
     ? $V{sumSteuer_Steuerart}.add($F{Detail} ? ($F{Steuer}==null ? BigDecimal.ZERO : $F{Steuer}) : BigDecimal.ZERO)
     : $V{sumSteuer_Steuerart}.add($F{Steuer})]]></variableExpression>
		<initialValueExpression><![CDATA[BigDecimal.ZERO]]></initialValueExpression>
	</variable>
