	<variable name="Umsatzsumme" class="java.lang.Double" resetType="Group" resetGroup="Gruppe" calculation="Sum">
		<variableExpression><![CDATA[$F{Satz}==null || $F{Umsatzgerundet} == null ?
new Double(0.00) :
(
 $F{Gruppe}.intValue() >= 3 && $F{Gruppe}.intValue() <= 5 ?
 $F{Umsatzgerundet}.doubleValue() :
 new Double(0.00)
)
/* Ist ganz speziell um die Umsatzsummen anzudrucken für div. Kontrollen */]]></variableExpression>
	</variable>
	<variable name="UMS_Gruppe" class="java.lang.Double" resetType="Group" resetGroup="Gruppe" calculation="Sum">
		<variableExpression><![CDATA[$F{Satz}==null || $F{Umsatzgerundet} == null ?
new Double(0.00) :
(
 $F{Gruppe}.intValue() != $V{Vorsteuer}.intValue() ?
 $F{Umsatzgerundet}.doubleValue() * $F{Satz}.doubleValue() / 100 :
 $F{Umsatz}.doubleValue() /* * -1 da nur Gruppen Summe für VST nicht invertieren */
)]]></variableExpression>
	</variable>
	<variable name="SteuerSumme" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{Satz}==null || $F{Umsatzgerundet} == null ?
new Double(0.00) :
(
 $F{Gruppe}.intValue() != $V{Vorsteuer}.intValue() ?
 $F{Umsatzgerundet}.doubleValue() * $F{Satz}.doubleValue() / 100.00 :
 $F{Umsatz}.negate().doubleValue()
)]]></variableExpression>
	</variable>
	<variable name="IG_Erwerb_Steuern" class="java.lang.Double" calculation="Sum">
		<variableExpression><![CDATA[$F{Satz}==null || $F{Umsatzgerundet} == null ?
new Double(0.00) :
(
 $F{Gruppe}.intValue() == $V{IG_Erwerb}.intValue() ?
 $F{Umsatzgerundet}.doubleValue() * $F{Satz}.doubleValue() / 100.00 :
 new Double(0.00)
)]]></variableExpression>
	</variable>
